default: &default
  adapter: mysql2
  encoding: utf8
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>

development:
  <<: *default
  database: fcsp_development
  #username: fcsp
  #password:
  #host: localhost
  #port: 3306

test:
  <<: *default
  database: fcsp_test
  #username: fcsp
  #password:
  #host: localhost
  #port: 3306

production:
  <<: *default
  database: fcsp_production
  username: fcsp
  password: <%= ENV['FCSP_DATABASE_PASSWORD'] %>
