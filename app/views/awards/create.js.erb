$('.list-award').html('<%= j render partial: "awards/list_award", locals: {awards: @awards} %>');
$('#award-modal').modal().hide();
$('.modal-backdrop').remove();
<% if @render.status == 200 %>
  $.growl.notice({message: '<%= @render.message %>'});
<% else %>
  $.growl.error({message: '<%= @render.message %>'});
<% end %>
